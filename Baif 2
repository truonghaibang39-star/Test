#include<bits/stdc++.h>
using namespace std;

void check() {
    string s;
    getline(cin, s); 
 	vector<int> count(10, 0); 
    for (int i = 0; i < s.length(); ++i) {
       if (s[i] >='0'&&s[i]<='9') { 
            int index = s[i] - '0';
            count[index]++;
        }
    }
    for (int i = 0; i < 10; ++i) {
        cout << count[i] << (i == 9 ? "" : " ");
    }
}

int main() {
    check();
    return 0;
}
